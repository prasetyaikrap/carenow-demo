{
  "name": "cadit-iot-application",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "build/server.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env --clear-screen=false server.ts",
    "build": "tsup",
    "start": "node --env-file=.env build/server.js",
    "lint": "next lint --max-warnings 0",
    "clean": "rm -rf node_modules && rm -rf dist && rm *.tsbuildinfo",
    "check-types": "tsc --noEmit",
    "drizzle:generate": "npx drizzle-kit generate --name=carenow-db",
    "drizzle:migrate": "npx drizzle-kit migrate"
  },
  "dependencies": {
    "auto-bind": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.14.1",
    "postgres": "^3.4.5",
    "ts-pattern": "^5.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.11.11",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "drizzle-kit": "^0.30.6",
    "tsup": "^8.3.5",
    "tsx": "^4.19.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20"
  }
}
